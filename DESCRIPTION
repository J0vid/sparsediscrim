    'dlda.r'
    'dqda.r'
    'joint-diagonalization.r'
    'rsdda.r'
    'sdlda.r'
    'sdqda.r'
    'stein-shrinkage.r'
    'simdiag.r'
    'estimates.r'
    'helper-intercept.r'
    'helper-simdiag2.r'
    'data-intraclass.r'
    'helper-transform.r'
    'data-block-autocorrelation.r'
    'helper-cov-mle.r'
    'helper-simdiag-fastsvd.r'
    'helper-shrinkage.r'
    'helper-mean-shrinkage.r'
    'data-parametric-bootstrap.r'
    'bhattacharyya.r'
    'helper-pct-change.r'
    'helper-fast-cov-eigen.r'
