    'helper-intercept.r'
    'data-intraclass.r'
    'data-block-autocorrelation.r'
    'helper-cov-mle.r'
    'helper-shrinkage.r'
    'helper-mean-shrinkage.r'
    'data-parametric-bootstrap.r'
    'helper-pct-change.r'
